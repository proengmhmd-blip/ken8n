{
  "$schema": "https://json.schemastore.org/package.json",
  "version": "2.4.4",
  "name": "ken8n-coder",
  "type": "module",
  "private": false,
  "description": "AI-powered n8n workflow creation agent built for the terminal",
  "author": "Ken Kai <ken@kenkai.dev>",
  "license": "MIT",
  "homepage": "https://github.com/kenkaiii/ken8n-coder",
  "repository": {
    "type": "git",
    "url": "https://github.com/kenkaiii/ken8n-coder"
  },
  "keywords": [
    "n8n",
    "workflow",
    "automation",
    "ai",
    "super-code",
    "terminal",
    "tui"
  ],
  "scripts": {
    "typecheck": "tsc --noEmit",
    "dev": "bun run --conditions=development ./src/index.ts"
